package aula07;

public class MetodoConstrutor {
	
	final String raca; // imutavel
	String nome; // instancia
	double renda;
	static double rendaTotal; // classe
	
	public MetodoConstrutor(String nome, double renda) {
		System.out.println("Iniciando MÃ©todo Construtor");
		this.raca = "Humano";
		this.nome = nome;
		this.renda = renda;
		rendaTotal = rendaTotal+renda;
	}
	
	public String toString() {
		return "MetodoConstrutor[\nraca=" + raca + "\nnome=" + nome + "\nrenda=" + renda + "\n]";
	}
	
}
