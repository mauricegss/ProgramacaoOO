package aula03;

public class CarroTeste {
	
	public static void main(String[] args) {
	
		//instanciacao de objetos da classe carro
		carro carro1 = new carro();
		carro carro2 = new carro();
		carro carro3 = new carro();
		
		// Atribuicao para cada carro
		carro1.modelo = "458 Italia";
		carro1.marca = "Ferrari";
		carro1.ano = 2015;
		carro1.placa = "ABC-1234";
		carro1.cor = "Vermelho";
		
		carro2.modelo = "459 Italia";
		carro2.marca = "Ferrari";
		carro2.ano = 2016;
		carro2.placa = "ABC-1235";
		carro2.cor = "Vermelho";
		
		carro3.modelo = "460 Italia";
		carro3.marca = "Ferrari";
		carro3.ano = 2017;
		carro3.placa = "ABC-1236";
		carro3.cor = "Branco";
		
		// Obter valores de cada objeto
		System.out.println("Carro 1:");
		System.out.println("Modelo:" + carro1.modelo);
		System.out.println("Carro 1:" + carro1.marca);
		System.out.println("Carro 1:" + carro1.ano);
		System.out.println("Carro 1:" + carro1.placa);
		System.out.println("Carro 1:" + carro1.ano);
		// ou... (mais facil n√©)
		carro2.printcarro();
	}

}
