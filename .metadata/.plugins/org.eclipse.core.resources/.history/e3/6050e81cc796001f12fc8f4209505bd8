package aula07;

public class MetodoConstrutor {
	
	final String raca;
	String nome; // instancia
	double renda;
	static double rendatotal; // classe
	
	public MetodoConstrutor(String n, double r) {
		System.out.println("Iniciando Metodo Construtor");
		raca = "Humano";
		nome = n;
		renda = r;
		rendatotal = rendatotal+renda;
	}
	
}
