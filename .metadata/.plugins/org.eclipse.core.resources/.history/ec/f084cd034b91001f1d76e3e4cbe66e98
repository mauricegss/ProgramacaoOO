package aula06;

public class Pessoa {
	
	final String raca = "Humano"; // imutável
	String nome; // instancia
	double renda;
	static double rendatotal;
	
	
	public void imprimir(int numero) {
		
		System.out.println("Nome: " + nome);
		System.out.println("Raça: " + raca);
		System.out.println("Renda: " + renda);
		
	}
	
}
